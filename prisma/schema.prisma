generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model ObservedRepo {
  id           String    @id @unique @default(uuid())
  url          String?
  status       String?   @default("ACTIVE")
  owner        String?
  name         String?
  pullRequests Int?
  openIssues   Int?
  license      String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @updatedAt
}
